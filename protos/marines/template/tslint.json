{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "lintOptions": {
    "typeCheck": true
  },
  "jsRules": {},
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": false,
    "trailing-comma": false,
    "semicolon": false
  },
  "rulesDirectory": []
}