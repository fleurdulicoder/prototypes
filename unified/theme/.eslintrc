{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true,
  },
  "rules": {
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "linebreak-style": "off",
    "camelcase": "off",
    "new-cap": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "prefer-template": "off",
    "no-tabs": "off",
  }
}
